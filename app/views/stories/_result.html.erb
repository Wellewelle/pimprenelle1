<% stories.each do |story| %>
<div class="autocomplete_result d-flex">
  <div class="testtaille">
    <% if story.photo.attached? %>
      <%= cl_image_tag story.photo.key, crop: :pad, class:"autocomplete_vignette", style:"height: 59px; width: 60px;"  %>
    <% else %>
      <img id="" class="autocomplete_vignette" style="height: 59px; width: 60px;" src="https://media.iletaitunehistoire.com//images/biblidcon_053i01.png">
    <% end %>
  </div>
  <div class="test2taille">
      <a class="autocomplete_story" href="/stories/<%=story.id%>"><%=story.title%></a>
  </div>
</div>
<% end %>
